<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<treeTransformer>
	<leafToSeqExtractorFormatter>
		<!-- designed to match <alignmentName>.<memberSourceName>.<memberSequenceID> -->
		<matchPattern>.+\.(.+)\.(.+)</matchPattern>
		<outputTemplate>source.name = '${g1}' and sequenceID = '${g2}'</outputTemplate>	
	</leafToSeqExtractorFormatter>
    <outputLeafTemplate>${renderProperty('gb_country_iso')}</outputLeafTemplate>
</treeTransformer>
