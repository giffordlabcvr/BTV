<genbankXmlPopulator>

	<gbFeature key="source">
		<!-- #### SEGMENT -->			
		<gbQualifier name="serotype">
			<propertyPopulator property="gb_serotype">
				<matchPattern>(\d+)</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
		<gbQualifier name="organism">
			<propertyPopulator property="gb_serotype">
				<matchPattern>(\d+)</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
		<gbQualifier name="isolate">
			<propertyPopulator property="gb_serotype">
				<matchPattern>BTV(?:[/\-])?(\d+)</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
		<gbQualifier name="strain">
			<propertyPopulator property="gb_serotype">
				<matchPattern>BTV(?:[/\-])?(\d+)</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
		<gbQualifier name="strain">
			<propertyPopulator property="gb_serotype">
				<matchPattern>serotype (\d+)</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
		<gbQualifier name="genotype">
			<propertyPopulator property="gb_serotype">
				<matchPattern>(\d+)</matchPattern>
				<outputTemplate>${g1}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>
	</gbFeature>
</genbankXmlPopulator>