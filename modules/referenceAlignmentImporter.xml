<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<blastFastaAlignmentImporter>
    <idClauseExtractorFormatter>
        <matchPattern>([a-zA-Z0-9\-_]*)</matchPattern>
        <outputTemplate>source.name = 'ncbi-refseqs' and sequenceID = '${g1}'</outputTemplate>
    </idClauseExtractorFormatter>
    <ignoreMissingSequences>true</ignoreMissingSequences>
</blastFastaAlignmentImporter>
