<nexusExporter>
	<freemarkerAnnotationGenerator>
		<annotationName>virus</annotationName>
		<template><![CDATA[<#if (sequence.source.name = 'ncbi-outgroup' && sequence.sequenceID?starts_with('AM7449')) || (sequence.source.name = 'ncbi-outgroup-fullgenomes' && sequence.sequenceID = 'ehdvFullGenome')>EHDV<#elseif (sequence.source.name = 'ncbi-outgroup' && sequence.sequenceID?starts_with('JQ0703')) || (sequence.source.name = 'ncbi-outgroup-fullgenomes' && sequence.sequenceID = 'patavFullGenome')>PATAV<#elseif sequence.isolate_segment?? && sequence.isolate_segment = 6>BTV${renderNestedProperty('sequence.isolate.seg6serotype')}<#else>BTV${renderNestedProperty('sequence.isolate.seg2serotype')}</#if>]]></template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>glue_clade_plus_cluster</annotationName>
		<template><![CDATA[<#list sequence.alignmentMemberships as memb><#if memb.alignment.name?startsWith('AL_')><#assign almt = memb.alignment.displayName!memb.alignment.name><#break></#if></#list>${almt!"-"}:${sequence.cluster_a!"-"}]]></template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>accession_country_isolateS2serotype_year</annotationName>
		<template>${renderNestedProperty('sequence.sequenceID')}/${renderNestedProperty('sequence.isolate.who_country.display_name')}/${renderNestedProperty('sequence.isolate.seg2serotype')}/${renderNestedProperty('sequence.isolate.collection_year')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>accession_country_isolateS6serotype_year</annotationName>
		<template>${renderNestedProperty('sequence.sequenceID')}/${renderNestedProperty('sequence.isolate.who_country.display_name')}/${renderNestedProperty('sequence.isolate.seg6serotype')}/${renderNestedProperty('sequence.isolate.collection_year')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>country_iso</annotationName>
		<template>${renderNestedProperty('sequence.isolate.who_country.id')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>country_display_name</annotationName>
		<template>${renderNestedProperty('sequence.isolate.who_country.display_name')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>region</annotationName>
		<template>${renderNestedProperty('sequence.isolate.who_country.who_region.id')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>sub_region</annotationName>
		<template>${renderNestedProperty('sequence.isolate.who_country.who_sub_region.id')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>host</annotationName>
		<template>${renderNestedProperty('sequence.isolate.host')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>isolate_display_name</annotationName>
		<template>${renderNestedProperty('sequence.isolate.display_name')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>year</annotationName>
		<template>${renderNestedProperty('sequence.isolate.collection_year')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>accession</annotationName>
		<template>${renderNestedProperty('sequence.sequenceID')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>seg2clade</annotationName>
		<template>${renderNestedProperty('sequence.isolate.seg2clade.displayName')}</template>
	</freemarkerAnnotationGenerator>
	<figtreeProperty type="integer" name="tipLabels.fontSize">13</figtreeProperty>
	<figtreeProperty name="tipLabels.colorAttribute">cluster_tbe_n85_p25</figtreeProperty>
	<figtreeProperty name="tipLabels.displayAttribute">accession_country_isolateS2serotype_year</figtreeProperty>
	<figtreeProperty type="integer" name="nodeLabels.fontSize">13</figtreeProperty>
	<figtreeProperty type="boolean" name="nodeLabels.isShown">true</figtreeProperty>
	<figtreeProperty name="nodeLabels.displayAttribute">transferBootstraps</figtreeProperty>
	<figtreeProperty type="boolean" name="nodeLabels.isShown">true</figtreeProperty>
	<figtreeProperty name="layout.layoutType">RADIAL</figtreeProperty>
</nexusExporter>