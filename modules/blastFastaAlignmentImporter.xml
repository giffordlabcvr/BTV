<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<blastFastaAlignmentImporter>
    <idClauseExtractorFormatter>
        <matchPattern>.*\.([a-zA-Z0-9\-_]*)\.([a-zA-Z0-9\-_]*)</matchPattern>
        <outputTemplate>source.name = '${g1}' and sequenceID = '${g2}'</outputTemplate>
    </idClauseExtractorFormatter>
</blastFastaAlignmentImporter>
