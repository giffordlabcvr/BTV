<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<blastProteinFastaAlignmentImporter>
	<idClauseExtractorFormatter>
		<matchPattern>.*\.(.*)\.(.*)</matchPattern>
		<outputTemplate>source.name = '${g1}' and sequenceID = '${g2}'</outputTemplate>
	</idClauseExtractorFormatter>
</blastProteinFastaAlignmentImporter>
