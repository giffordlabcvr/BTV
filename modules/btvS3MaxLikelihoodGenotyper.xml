<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<maxLikelihoodGenotyper>
    <maxLikelihoodPlacerModuleName>btvS3MaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
    <cladeCategory>
        <name>species</name>
        <displayName>Species</displayName>
        <whereClause>clade_category = 'species'</whereClause>
        <distanceScalingExponent>-3.0</distanceScalingExponent>
        <distanceCutoff>2.00</distanceCutoff>
        <internalDistanceCutoff>4.00</internalDistanceCutoff>
        <finalCladeCutoff>80.0</finalCladeCutoff>
    </cladeCategory>
    <cladeCategory>
        <name>genogroup</name>
        <displayName>Genogroup</displayName>
        <whereClause>clade_category = 'genogroup'</whereClause>
        <distanceScalingExponent>-3.0</distanceScalingExponent>
        <distanceCutoff>0.20</distanceCutoff>
        <internalDistanceCutoff>0.40</internalDistanceCutoff>
        <finalCladeCutoff>80.0</finalCladeCutoff>
    </cladeCategory>
    <useSingleReference>true</useSingleReference>
</maxLikelihoodGenotyper>
